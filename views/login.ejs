<%- include('partials/header') %>
<div class="container">
    <div class="row justify-content-center"id="loginRow">
        <div class="col-12 col-md-12 text-center">
            <h3>Login</h3>
        </div>
    </div>
</div>
<div class="row justify-content-center"id="loginForm">
    <div class="col-6 text-center">
        <form action="/login" method="POST">
            
            <input class="form-group form-control"type="text"name="username"placeholder="username">
            <input class="form-group form-control"type="password"name="password"placeholder="password">
            
            <button class="btn btn-sm btn-warning">Login</button>
            </form>
    </div>
</div>
<%- include('partials/footer') %>